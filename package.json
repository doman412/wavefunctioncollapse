{
  "name": "wavefunctioncollapse",
  "version": "0.9.0",
  "description": "Javascript port of https://github.com/mxgmn/WaveFunctionCollapse",
  "main": "dist/wfc.umd.js",
  "module": "dist/wfc.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "run-p dev:tsc dev:vite",
