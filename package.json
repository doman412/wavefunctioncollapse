    "LICENSE",
    "README.md"
  ],
  "author": "doman412",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doman412/wavefunctioncollapse/issues"
  },
  "homepage": "https://github.com/doman412/wavefunctioncollapse#readme",
  "devDependencies": {
    "jimp": "0.16.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  }
}
